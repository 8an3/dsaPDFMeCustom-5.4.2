{
  "name": "root",
  "version": "0.0.0",
  "private": true,
  "author": "hand-dot",
  "license": "MIT",
  "description": "TypeScript base PDF generator and React base UI. Open source, developed by the community, and completely free to use under the MIT license!",
  "homepage": "https://pdfme.com",
  "repository": "git@github.com:hand-dot/pdfme-beta.git",
  "bugs": {
    "url": "https://github.com/hand-dot/pdfme-beta/issues"
  },
  "workspaces": [
    "packages/*",
    "website"
  ],
  "scripts": {
    "build": "npm run common:build && npm run ui:build && npm run generator:build",
    "linttest": "npm run common:lint && npm run common:test && npm run ui:lint && npm run ui:test && npm run generator:lint && npm run generator:test",
    "common:build": "yarn workspace @pdfme/common build",
    "common:develop": "yarn workspace @pdfme/common develop",
    "common:lint": "yarn workspace @pdfme/common lint",
    "common:test": "yarn workspace @pdfme/common test",
    "generator:build": "yarn workspace @pdfme/generator build",
    "generator:develop": "yarn workspace @pdfme/generator develop",
    "generator:lint": "yarn workspace @pdfme/generator lint",
    "generator:test": "yarn workspace @pdfme/generator test",
    "ui:build": "yarn workspace @pdfme/ui build",
    "ui:develop": "yarn workspace @pdfme/ui develop",
    "ui:lint": "yarn workspace @pdfme/ui lint",
    "ui:test": "yarn workspace @pdfme/ui test",
    "website:build": "yarn workspace @pdfme/website build",
    "website:develop": "yarn workspace @pdfme/website start",
    "website:lint": "yarn workspace @pdfme/website typecheck"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-loader": "^8.2.2",
    "prettier": "^2.3.2",
    "jest": "^27.4.7",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.5",
    "ts-prune": "^0.10.3",
    "typescript": "^4.5.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.51.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.7.2"
  },
  "resolutions": {
    "jest": "^27.4.7"
  }
}
